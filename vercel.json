{
  "version": 2,
  "builds": [
    {
      "src": "next.config.js",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/users/(.*)",
      "dest": "/api/users/$1"
    },
    {
      "src": "/api/users/register",
      "dest": "/api/users/register"
    },
    {
      "src": "/api/codes/(.*)",
      "dest": "/api/codes/$1"
    },
    {
      "src": "/api/executeCode",
      "dest": "/api/executeCode"
    },
    {
      "src": "/api/score",
      "dest": "/api/score"
    },
    {
      "src": "/api/contributors/(.*)",
      "dest": "/api/contributors/$1"
    },
    {
      "src": "/api/viewcount/(.*)",
      "dest": "/api/viewcount/$1"
    },
    {
      "src": "/api/user",
      "dest": "/api/user"
    },
    {
      "src": "/api/category",
      "dest": "/api/category/index"
    }
  ]
}
